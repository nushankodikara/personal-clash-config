
port: 7890
socks-port: 7891
redir-port: 7892
mixed-port: 7893
mode: rule
log-level: silent
allow-lan: true
external-controller: 0.0.0.0:9090
bind-address: "*"
ipv6: false
dns:
  enable: true
  ipv6: false
  enhanced-mode: redir-host
  listen: 0.0.0.0:7894
  fallback-filter:
    geoip: false
    ipcidr:
    - 240.0.0.0/4
  nameserver:
    - https://puredns.org/dns-query
    - tls://puredns.org:853
  fallback:
    - tcp://108.137.44.39
    - tcp://108.137.44.9
    - 108.137.44.39
    - 108.137.44.9
tun:
  enable: true
  stack: system 
  macOS-auto-route: true
  macOS-auto-detect-interface: true
  dns-hijack:
    - tcp://108.137.44.39:53
experimental:
  interface-name: en0

proxies:
  - {name: 443 Vmess Singapore HE-1 Limit 2023-12-29, server: sg-1.hihu.net, port: 443, type: vmess, uuid: 79035950-a08e-11ee-b838-205c6d5f5d78, sni: store.steampowered.com, alterId: 0, cipher: auto, tls: 1, skip-cert-verify: true, udp: true, network: ws, ws-opts: {path: /kuf8g51j, headers: {Host: store.steampowered.com}}}
  - {name: 443 Vmess Indonesia Herza Limit 2023-12-29, server: id-1.hihu.net, port: 443, type: vmess, uuid: 8855f890-a08e-11ee-b231-205c6d5f5d78, sni: store.steampowered.com, alterId: 0, cipher: auto, tls: 1, skip-cert-verify: true, udp: true, network: ws, ws-opts: {path: /an27lqo1, headers: {Host: store.steampowered.com}}}
  - {name: SG+VMESS-WS(2023-12-29), server: sg3.socifiles.com, port: 443, type: vmess, uuid: dca999e0-a08e-11ee-b5ed-1239d0255272, sni: , alterId: 0, cipher: auto, tls: 1, skip-cert-verify: true, udp: true, network: ws, ws-opts: {path: /vmws, headers: {Host: store.steampowered.com }}}

proxy-groups:
  - name: sshkit-generator
    type: load-balance
    url: 'http://www.gstatic.com/generate_204'
    interval: 300
    strategy: consistent-hashing
    proxies:
      - 443 Vmess Singapore HE-1 Limit 2023-12-29
      - 443 Vmess Indonesia Herza Limit 2023-12-29
      - SG+VMESS-WS(2023-12-29)

rules:
 - MATCH,sshkit-generator
